{
  "name": "@dxc-technology/halstack-angular-hal",
  "version": "0.0.0",
  "scripts": {
    "build-lib": "ng build && npm run copy-md-lib",
    "build-lib-prod": "ng build --prod && npm run copy-md-lib",
    "package": "cd ./../../dist/diaas-angular-cdk-hal && npm pack",
    "copy-md-lib": "cp ./../../README.md ./../../dist/diaas-angular-cdk-hal/",
    "generate-lib": "npm run build-lib && npm run package",
    "generate-lib-prod": "npm run build-lib-prod && npm run package",
    "test": "jest --config ./jest.lib.config.js",
    "test:coverage": "jest --config ./jest.lib.config.js --coverage",
    "build-lib:watch": "ng build --watch"
  },
  "peerDependencies": {
    "@angular/animations": "10.2.0",
    "@angular/cdk": "10.2.5",
    "@angular/common": "10.2.0",
    "@angular/compiler": "10.2.0",
    "@angular/core": "10.2.0",
    "@angular/forms": "10.2.0",
    "@angular/material": "10.2.5",
    "@angular/material-moment-adapter": "10.2.5",
    "@angular/platform-browser": "10.2.0",
    "@angular/platform-browser-dynamic": "10.2.0",
    "@angular/router": "10.2.0",
    "@dxc-technology/halstack-client": "^1.1.5",
    "@dxc-technology/halstack-angular": "0.0.0-bfdd610",
    "rxjs": "~6.5.2"
  },
  "dependencies": {
    "@dxc-technology/halstack-angular": "0.0.0-bfdd610",
    "@dxc-technology/halstack-client": "^1.1.5",
